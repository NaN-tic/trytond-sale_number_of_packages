<tryton>
    <data group="1">
      <record model="ir.message" id="package_required">
          <field name="text">Package required for move "%(move)s".</field>
      </record>
      <record model="ir.message" id="number_of_packages_required">
          <field name="text">Number of packages required for move "%(move)s".</field>
      </record>
      <record model="ir.message" id="package_qty_required">
          <field name="text">Quantity by Package is required for package "%(package)s" of sale line "%(record)s".</field>
      </record>
      <record model="ir.message" id="invalid_quantity_number_of_packages">
          <field name="text">The quantity of sale line "%(sale)s" do not correspond to the number of packages.</field>
      </record>
      <record model="ir.message" id="unexpected_number_of_packages_divider_multiplier">
          <field name="text">The Number of Packages Divider of lot "%(lot)s" doesn\'t
                    corresponds with the Multiplier.</field>
      </record>

    </data>
</tryton>
